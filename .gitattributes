# Auto detect text files and perform LF normalization
* text=auto
int a = 4;
int b = 6;
if(a + b > 12)
(
    Console.WriteLine("The answer is greater than 12");
)
else if (a + b > 8)
{
    Console.WriteLine("The answer is greater than 8");
}
else
{
    Console.WriteLine("The answer is not greater than 8");
}